@using Final_Project.Models;
@{
   Editor preEditData=(Editor)Session["pre-editedData"];
   
   
}
<!DOCTYPE html>
<!--
Item Name: Elisyam - Web App & Admin Dashboard Template
Version: 1.3
Author: SAEROX

** A license must be purchased in order to legally use this template for your project **
-->
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Elisyam - Forms Validation</title>
    <meta name="description" content="Elisyam is a Web App and Admin Dashboard Template built with Bootstrap 4">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Google Fonts -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Montserrat:400,500,600,700", "Noto+Sans:400,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!-- Favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href="~/Content/assets/img/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Content/assets/img/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/assets/img/favicon-16x16.png">
    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/Content/assets/vendors/css/base/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/assets/vendors/css/base/elisyam-1.2.min.css">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<body id="page-top">
    <!-- Begin Preloader -->
    <div id="preloader">
        <div class="canvas">
            <img src="~/Content/assets/img/logo.png" alt="logo" class="loader-logo">
            <div class="spinner"></div>
        </div>
    </div>
    <!-- End Preloader -->
    <div class="page">
        <!-- Begin Header -->
        <!-- End Header -->
        <!-- Begin Page Content -->
        <div class="page-content d-flex  align-self-lg-center">

            <!-- End Left Sidebar -->
            <div class="content-inner">
                <div class="container-fluid">
                    <!-- Begin Page Header-->
                    <div class="row">
                        <div class="page-header">
                            <div class="d-flex align-items-center">
                                <h2 class="page-header-title">Edit Management System</h2>
                                <div>
                                    <ul class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="db-default.html"><i class="ti ti-home"></i></a></li>
                                        <li class="breadcrumb-item"><a href="#">Nexum</a></li>
                                        <li class="breadcrumb-item active">Edit Management System</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Page Header -->
                    <div class="row flex-row">
                        <div class="col-xl-12">
                            <!-- Form -->
                            <div class="widget has-shadow">
                                <div class="widget-header bordered no-actions d-flex align-items-center">
                                    <h4>@preEditData.MsName</h4>
                                </div>
                                <div class="widget-body">
                                    @using (Html.BeginForm("EditMS", "Creator"))
                                    {
                                    <form class="needs-validation" novalidate>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Name</label>
                                            <div class="col-lg-5">
                                                <input type="text" class="form-control" name="name" placeholder=@preEditData.MsName>
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Branch Name</label>
                                            <div class="col-lg-5">
                                                <input type="text" class="form-control" name="branch" placeholder=@preEditData.BranchName>
                                            </div>
                                        </div>
                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Address Name</label>
                                            <div class="col-lg-5">
                                                <input type="text" class="form-control" name="address" placeholder=@preEditData.Address>
                                            </div>
                                        </div>

                                        <div class="form-group row d-flex align-items-center mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Phone Number</label>
                                            <div class="col-lg-5">
                                                <div class="input-group">
                                                    <span class="input-group-addon addon-primary">
                                                        <i class="la la-phone"></i>
                                                    </span>
                                                    <input type="tel" class="form-control" name="number" placeholder=@preEditData.PhoneNumber>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group row mb-5">
                                            <label class="col-lg-4 form-control-label d-flex justify-content-lg-end">Choose Theme *</label>
                                            <div class="col-lg-1">
                                                <div class="custom-control custom-radio styled-radio mb-3">
                                                    <input class="custom-control-input" type="radio" name="options1" id="opt-01" required>
                                                    <label class="custom-control-descfeedback" for="opt-01">Dark</label>

                                                    <div class="invalid-feedback">
                                                        Toggle this custom radio
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-1">  
                                                <div class="custom-control custom-radio styled-radio mb-3">
                                                    <input class="custom-control-input" type="radio" name="options2" id="opt-02" required>
                                                    <label class="custom-control-descfeedback" for="opt-02">Light</label>
                                                    <div class="invalid-feedback">
                                                        Or toggle this other custom radio
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="text-right">
                                           
                                            <button class="btn btn-gradient-01" type="submit">Next</button>
                                            <button class="btn btn-shadow" type="reset">Reset</button>
                                        </div>
                                    </form>
                                    }
                                </div>
                            </div>
                            <!-- End Form -->
                        </div>
                    </div>
                    <!-- End Row -->
                </div>
                <!-- End Container -->
                <!-- Begin Page Footer-->
                <footer class="main-footer">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 d-flex align-items-center justify-content-xl-start justify-content-lg-start justify-content-md-start justify-content-center">
                            <a class="btn btn-gradient-01 text-white" href="#">Support Center</a>
                        </div>
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 d-flex align-items-center justify-content-xl-end justify-content-lg-end justify-content-md-end justify-content-center">
                            <ul class="nav">
                                <li class="nav-item">
                                    <a class="nav-link" href="documentation.html">Documentation</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="changelog.html">Changelog</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </footer>
                <!-- End Page Footer -->
                <a href="#" class="go-top"><i class="la la-arrow-up"></i></a>
                
            </div>
        </div>
        <!-- End Page Content -->
    </div>
    <!-- Begin Vendor Js -->
    <script src="~/Content/assets/vendors/js/base/jquery.min.js"></script>
    <script src="~/Content/assets/vendors/js/base/core.min.js"></script>
    <!-- End Vendor Js -->
    <!-- Begin Page Vendor Js -->
    <script src="~/Content/assets/vendors/js/nicescroll/nicescroll.min.js"></script>
    <script src="~/Content/assets/vendors/js/app/app.min.js"></script>
    <!-- End Page Vendor Js -->
    <!-- Begin Page Snippets -->
    <script src="~/Content/assets/js/components/validation/validation.min.js"></script>
    <!-- End Page Snippets -->
</body>
</html>